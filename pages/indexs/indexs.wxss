
/* pages/indexs/indexs.wxss */
.banner{
  width: 100%;
  height: 300rpx;
}
.banner image{
  display: inline-block;
  overflow: hidden;
  height: 100%; /*562.5rpx;*/
  border-radius: 15rpx;
  background-color: #fff;
  box-shadow: 0 10rpx 15rpx rgba(0, 0, 0, 0.1);
  position: relative;
  width: 100%;
}
.isclass{
  overflow:hidden;
  display: block;
  padding:20rpx 0;
}
.classify{
  text-align:center;
  font-size: 28rpx;
}
.classify image{
  height: 80rpx;
  width:80rpx;
}

.message{
  height:50rpx;
  padding: 10rpx;
  overflow:hidden;
  background: #fff;
}
.message .ismessage{
  font-size: 14px;
  line-height: 50rpx;
  padding-left:20rpx;
  margin-left:10rpx;
  border-radius:10rpx;
  border: 1px solid #eee;
  float:left;
  height:50rpx;
  width:70%;
}
.swiper_item{
  overflow: visible;
  border-radius: 50%;
  text-align: center;
}
.message .content{
  font-size: 24rpx;
  width:100%;
  height:100%;
  display:block;
  float:left;
  white-space: norwap;
  overflow:hidden;
  text-overflow:ellipsis;
}
#index_content{
  padding-top: 20rpx;
  background: #eee;
}
/* 联系 */
.contact{
  margin-top: 20rpx;
  background: #fff;
  padding: 26rpx 30rpx;
  overflow: hidden;
}
.contact .contact_img{
  height: 90rpx;
  width: 90rpx;
  float: left;
}
.contact .contact_img image{
  height: 100%;
  width: 100%;
}
.contact .contact_text{
  padding-left: 24rpx;
  width: 316rpx;
  float: left;
}
.contact .contact_text .head{
  font-weight: bold;
  font-size: 26rpx;
  height: 26rpx;
  line-height: 26rpx;
}
.contact .contact_text .detail{
  font-size: 22rpx;
  color: #ccc;
  height: 26rpx;
  padding-top: 17rpx;
  line-height: 26rpx;
}
.contact .contact_phone{
  height: 90rpx;
  float: right;
  padding-top: 10rpx;
}
.contact .contact_phone .play_phone,.contact .contact_phone .play_wx{
  padding: 0;
  margin: 0;
  background: #fff;
  outline: none;
  border-radius: 50%;
  display: block;
  height: 69rpx;
  width: 69rpx;
  margin-left: 20rpx;
  float: right;
}
.contact .contact_phone .play_phone image,.contact .contact_phone .play_wx image{
  height: 100%;
  width: 100%;
}
/* 优惠券 */
.coupon,.hotgoods,.recommend{
  margin-top: 20rpx;
  padding: 0 26rpx;
  background: #fff;
}
.coupon .coupon_top,.hotgoods .hotgoods_top,.recommend .recommend_top{
  height: 27rpx;
  width: 100%;
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
}
.coupon .coupon_top .img,.hotgoods .hotgoods_top .img,.recommend .recommend_top .img{
  margin: 0 auto;
  width: 246rpx;
  height: 100%;
}
.coupon .coupon_top image,.hotgoods .hotgoods_top image,.recommend .recommend_top image{
  height: 100%;
  width: 100%;
  vertical-align: top;
}
.coupon .coupon_content,.hotgoods .hotgoods_top_content{
  overflow: hidden;
  padding: 12rpx 0;
}
.coupon_content_left{
  height: 110rpx;
  width: 162rpx;
  background: red;
  border-radius: 10rpx;
  float: left;
}
.coupon_content_left .money1{
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  line-height: 68rpx;
  height: 68rpx;
  border-bottom: 1px dashed #fff;
}
.coupon_content_left .money2{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  color: #fff;
  text-align: center;
}
.coupon_content_middle{
  float: left;
  padding-left: 30rpx;

}
.coupon_content_middle .yhq{
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;

}
.coupon_content_middle .date{
  font-size: 20rpx;
  color: #999;
}
.coupon_content_right {
  float: right;
  width: 150rpx;
  padding: 0 6rpx;
}
.coupon_content_right .draw{
  height: 54rpx;
  width: 150rpx;
  line-height: 54rpx;
  border: 1px solid #787878;
  text-align: center;
  border-radius: 27rpx;
  font-size: 22rpx;
  margin: 8rpx 0;
  background: #f8f8f8;
}
/* 爆款 */
.people_draw{
  text-align: center;
  font-size: 18rpx;
  height: 36rpx;
  line-height: 36rpx;
}
.hotgoods .hotgoods_top .more{
  position: absolute;
  top: 20rpx;
  right:0;
  font-size: 20rpx;
  height: 26rpx;
  line-height: 26rpx;
}
.hotgoods .hotgoods_top .more image{
  height: 26rpx;
  width: 26rpx;
}
.hotgoods_top_content .img{
  width: 100%;
  height: 514rpx;
}
.hotgoods_top_content image{
  height: 100%;
  width: 100%;
}
.hotgoods_top_content{
  text-align: center;
}
.hotgoods_top_content .text1{
  font-size: 28rpx;
  height: 54rpx;
  line-height: 54rpx;
}
.hotgoods_top_content .text2{
  height: 58rpx;
  line-height: 58rpx;
  color: #ff7300;
  font-size: 30rpx;
}

